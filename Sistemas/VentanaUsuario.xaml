<Window x:Class="Sistemas.Usuario_menu_principal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Sistemas"
        mc:Ignorable="d"
        Title="" Height="650"  Width="800" MinWidth="300" >
	<Grid x:Name="grid" Margin="20,30">
		
		<Grid.RowDefinitions>
			<RowDefinition Name="fila1" Height="*" ></RowDefinition>
			<RowDefinition Name="fila2" Height="*"></RowDefinition>
			<RowDefinition Name="fila3" Height="*"></RowDefinition>
			<RowDefinition Name="fila4" Height="*"></RowDefinition>
		</Grid.RowDefinitions>
		<Grid.ColumnDefinitions>
			<ColumnDefinition Name="columna1" Width="*" ></ColumnDefinition>
			<ColumnDefinition Name="columna2" Width="*"></ColumnDefinition>
			<ColumnDefinition Name="columna3" Width="*"></ColumnDefinition>
			<ColumnDefinition Name="columna4" Width="*"></ColumnDefinition>
		</Grid.ColumnDefinitions>
		<Label  x:Name="nombre" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" Background="#00000000" FontSize="18"></Label>
		
		<Frame Name="contendorPaginas" Grid.ColumnSpan="4" Grid.RowSpan="4" NavigationUIVisibility="Hidden" Content=""/>
	</Grid>
</Window>
